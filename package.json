{
  "name": "happy-backend",
  "version": "1.0.0",
  "description": "API Rest Full Happy",
  "scripts": {
    "test": "jest --passWithNoTests --runInBand --silent --noStackTrace",
    "test:staged": "npm run test -- --findRelatedTests",
    "test:ci": "npm run test -- --coverage",
    "test:coveralls": "npm run test:ci && coveralls < coverage/lcov.info",
    "test:unit": "npm run test -- --watch -c jest-unit-config.js",
    "test:integration": "npm run test -- --watch -c jest-integration-config.js",
    "build": "rimraf dist && tsc -p build.json",
    "start:dev": "sucrase-node src/main/server.ts",
    "start:prod": "node dist/main/server.js",
    "docker:up": "npm run build && docker-compose up -d",
    "docker:down":"docker-compose down"
  },
  "keywords": [],
  "author": "Carlos Daniel",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/faker": "^5.1.4",
    "@types/jest": "^26.0.15",
    "@types/module-alias": "^2.0.0",
    "@types/node": "^14.14.9",
    "@types/sqlite3": "^3.1.6",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "coveralls": "^3.1.0",
    "eslint": "^7.14.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "faker": "^5.1.0",
    "git-commit-msg-linter": "^3.0.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.1",
    "rimraf": "^3.0.2",
    "sucrase": "^3.16.0",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": "12.x"
  },
  "dependencies": {
    "express": "^4.17.1",
    "sqlite": "^4.0.17",
    "sqlite3": "^5.0.0",
    "module-alias": "^2.2.2",
    "mysql2": "^2.2.5",
    "uuid": "^8.3.1",
    "validator": "^13.1.17"
  }
}
